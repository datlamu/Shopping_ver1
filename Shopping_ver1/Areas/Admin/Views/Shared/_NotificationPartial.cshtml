@if (TempData["Success"] != null)
{
	<script>
		Swal.fire({
			position: 'top-end',
			icon: 'success',
			title: @Html.Raw(Json.Serialize(TempData["success"])),
			showConfirmButton: false,
			timer: 1500
		});
	</script>
}

@if (TempData["Error"] != null)
{
	<script>
		Swal.fire({
			position: 'top-end',
			icon: 'error',
			title: @Html.Raw(Json.Serialize(TempData["error"])),
			showConfirmButton: false,
			timer: 1500
		});
	</script>
}